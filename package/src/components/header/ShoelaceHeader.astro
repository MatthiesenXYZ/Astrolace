---
// Import the Shoelace styles
import "@shoelace-style/shoelace/dist/themes/light.css"
import "@shoelace-style/shoelace/dist/themes/dark.css"

---


<script>
    // Import the Shoelace component library
    import "@shoelace-style/shoelace/dist/shoelace.js"
</script>

<script>
    // Import the Shoelace default icon library (Bootstrap Icons)
    import { registerIconLibrary } from '@shoelace-style/shoelace/dist/utilities/icon-library.js';
  
    // Register the default icon library
    registerIconLibrary('default', {
        resolver: name => `https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/icons/${name}.svg`
    });
</script>